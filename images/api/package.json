{
  "scripts": {
    "test-watch": "jest --watchAll --detectOpenHandles",
    "test": "jest --detectOpenHandles",
    "dev": "npx knex migrate:latest --cwd ./src/db && npx knex seed:run --cwd ./src/db && npm run test-watch",
    "start": "npx knex migrate:latest --cwd ./src/db && npm run test && npm run test-watch",
    "migrate": "npx knex migrate:latest --cwd src/db"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "knex": "^3.0.1",
    "mongodb": "^6.1.0",
    "mysql": "^2.18.1",
    "mysql2": "^3.6.2",
    "nodemon": "^3.0.1",
    "pg": "^8.11.3",
    "webpack": "^5.89.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.3",
    "@babel/preset-env": "^7.23.3",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0"
  }
}
